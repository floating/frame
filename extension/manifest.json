{
  "name": "Frame",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Frame",
  "homepage_url": "https://github.com/floating/frame",
  "background": {
    "scripts": ["index.js"]
  },
  "browser_action": {
    "default_title": "Frame"
  },
  "content_scripts" : [{
    "matches": ["file://*/*", "http://*/*", "https://*/*"],
    "js": ["inject.js"],
    "run_at": "document_start"
  }],
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  }
}
