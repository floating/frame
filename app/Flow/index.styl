html, body
  margin 0
  border 0
  font-family 'MainFont'
  font-weight 300
  -webkit-app-region no-drag
  overflow hidden
  color var(--outerspace)
  background var(--ghostB)

div
  user-select none
  cursor default

::-webkit-scrollbar
  width 0px
  background transparent


.dapps
  width 520px
  height 330px
  overflow-y scroll
  overflow-x hidden
  background var(--ghostC)

  .bar
    background var(--ghostD)
    position relative
    height 60px
    width calc(100% - 40px)
    margin 20px 20px 0px 20px
    box-size border-box
    border-radius 8px
    // box-shadow 0px 9px 10px rgba(24, 24, 124, 0.1)

    input 
      position absolute
      top 0 
      right 0
      bottom 0
      left 0
      width 100%
      height 100%
      outline none
      border none 
      background transparent
      padding 0px 20px
      font-family 'MainFont'
      color var(--outerspace08)
      font-size 28px
    
  .dappWrap
    padding 0px 10px 10px 0px
    display flex
    flex-wrap wrap
    margin 0px 0px 100px 0px

    .dapp
      width 80px
      height 110px 
      min-width 80px
      min-height 110px
      margin 20px 0px 0px 20px

      .dappIcon
        width 80px
        height 80px 
        min-width 80px
        min-height 80px 
        background var(--ghostD)
        border-radius 12px
        cursor pointer

      .dappName
        width 80px
        height 30px 
        min-width 80px
        min-height 30px 
        display flex
        justify-content center
        align-items center
        cursor pointer
        font-size 14px

