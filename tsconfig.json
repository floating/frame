{
  "compilerOptions": {
    "target": "es5",                     
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "rootDir": "main",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "rootDir": ".",
    "outDir": ".",
    "resolveJsonModule": true,
    "composite": true,
    "types": ["node", "Long"],
    "typeRoots": ["./node_modules/@types"]
  },
  "files": [
    "package.json"
  ]
}
