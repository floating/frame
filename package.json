{
  "name": "Frame",
  "version": "0.0.5-alpha.1",
  "description": "A cross-platform Ethereum interface",
  "main": "main",
  "scripts": {
    "start": "npm run launch",
    "alpha": "npm run launch",
    "dev": "cross-env NODE_ENV=development electron ./main",
    "launch": "cross-env NODE_ENV=production electron ./main",
    "build": "npm install && electron-rebuild",
    "build:app": "npm run build && mkdirp ./build && electron-packager ./ --out=./build --overwrite",
    "build:app:mac": "npm run build && mkdirp ./build && electron-packager ./ --out=./build --overwrite",
    "build:app:linux": "npm run build && mkdirp ./build && electron-packager ./ --out=./build --overwrite --icon=./asset/png/FrameAppIcon.png && electron-installer-debian --src build/Frame-linux-x64 --dest build --arch amd64",
    "build:app:windows": "npm run build && mkdirp ./build && electron-packager ./ --out=./build --overwrite --icon=./asset/ico/FrameAppIcon.ico",
    "electron-rebuild": "electron-rebuild",
    "test": "standard && jest",
    "local:rpc": "ganache-cli --account=\"0x2d6945dbddb8dcf5492004e6f720f8e971196ff61a61c4be99714ebc71e06c00,  5000000000000000000000\" --account=\"0xaef6a68a47c1628081e4e6df195f5f712ae4eb7da332a6d74dca06ae32a3e7ae,5000\""
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ]
  },
  "author": "Jordan Muir",
  "dependencies": {
    "@ledgerhq/hw-app-eth": "^4.13.0",
    "@ledgerhq/hw-transport-node-hid": "^4.13.0",
    "auto-launch": "^5.0.5",
    "babel": "^6.23.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bip32-path": "^0.4.2",
    "cross-env": "^5.1.5",
    "electron-positioner": "^3.0.0",
    "electron-store": "^2.0.0",
    "ethereumjs-tx": "^1.3.4",
    "ethereumjs-util": "^5.2.0",
    "node-hid": "^0.7.2",
    "octicons": "^7.2.0",
    "opn": "^5.3.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-restore": "^0.2.1",
    "react-transition-group": "^1.2.1",
    "stylus": "^0.54.5",
    "trezor.js": "^6.15.19",
    "usb-detection": "^3.0.3",
    "uuid": "^3.2.1",
    "web3": "1.0.0-beta.34",
    "web3-providers-http": "1.0.0-beta.34",
    "web3-providers-ipc": "1.0.0-beta.34",
    "web3-providers-ws": "1.0.0-beta.34",
    "websocket": "^1.0.25"
  },
  "devDependencies": {
    "electron": "^2.0.2",
    "electron-packager": "^12.0.1",
    "electron-rebuild": "^1.7.3",
    "ganache-cli": "^6.1.0",
    "jest": "^23.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.7.1",
    "standard": "^11.0.1"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^0.8.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": "github:floating/frame"
}
