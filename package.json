{
  "name": "Frame",
  "version": "0.0.5-alpha.3",
  "description": "An Ethereum interface",
  "main": "main",
  "build": {
    "appId": "sh.frame.app",
    "productName": "Frame",
    "dmg": {
      "title": "Frame"
    },
    "linux": {
      "icon": "./asset/png/FrameAppIcon.png",
      "target": [
        "AppImage",
        "deb",
        "snap",
        "tar.gz"
      ]
    },
    "win": {
      "icon": "./asset/ico/FrameAppIcon.ico"
    }
  },
  "scripts": {
    "start": "npm run launch",
    "bundle": "rm -rf bundle && parcel build app/tray.html --public-url ./ --out-dir bundle --target=electron",
    "watch": "rm -rf bundle && parcel watch app/tray.html --public-url ./ --out-dir bundle --target=electron",
    "dev": "cross-env NODE_ENV=development electron ./main",
    "launch": "npm run bundle && cross-env NODE_ENV=production electron ./main",
    "alpha": "npm run launch",
    "build": "npm run bundle && electron-builder",
    "release": "build",
    "postinstall": "electron-builder install-app-deps",
    "test": "standard && jest",
    "local:rpc": "ganache-cli --account=\"0x2d6945dbddb8dcf5492004e6f720f8e971196ff61a61c4be99714ebc71e06c00,  5000000000000000000000\" --account=\"0xaef6a68a47c1628081e4e6df195f5f712ae4eb7da332a6d74dca06ae32a3e7ae,5000\""
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/bundle/",
      "/dist/"
    ]
  },
  "author": {
    "name": "Jordan Muir",
    "email": "jordan@frame.sh",
    "url": "https//frame.sh"
  },
  "dependencies": {
    "@ledgerhq/hw-app-eth": "^4.13.0",
    "@ledgerhq/hw-transport-node-hid": "^4.13.0",
    "auto-launch": "^5.0.5",
    "babel": "^6.23.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bip32-path": "^0.4.2",
    "cross-env": "^5.1.5",
    "electron-log": "^2.2.16",
    "electron-positioner": "^3.0.0",
    "electron-store": "^2.0.0",
    "electron-updater": "^2.23.3",
    "ethereumjs-tx": "^1.3.4",
    "ethereumjs-util": "^5.2.0",
    "node-hid": "^0.7.2",
    "octicons": "^7.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-restore": "^0.2.1",
    "react-transition-group": "^1.2.1",
    "trezor.js": "^6.17.5",
    "usb-detection": "^3.0.3",
    "uuid": "^3.2.1",
    "web3": "1.0.0-beta.34",
    "web3-providers-http": "1.0.0-beta.34",
    "web3-providers-ipc": "1.0.0-beta.34",
    "web3-providers-ws": "1.0.0-beta.34",
    "ws": "^5.2.1"
  },
  "devDependencies": {
    "electron": "^2.0.2",
    "electron-builder": "^20.18.0",
    "electron-hot-loader": "^0.8.0",
    "ganache-cli": "^6.1.0",
    "jest": "^23.0.0",
    "mkdirp": "^0.5.1",
    "parcel-bundler": "^1.9.4",
    "standard": "^11.0.1",
    "stylus": "^0.54.5"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": "github:floating/frame"
}
