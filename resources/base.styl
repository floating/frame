  animation-delay 0s

.cardHide
  opacity 0
  pointer-events none

.glitch
  position absolute
  top 0
  right 0
  bottom 0
  left 0
  display flex
  justify-content center
  align-items center
  color var(--outerspace)

.line
  display none

.lastLine
  display block
  opacity 1

.glitchOn
  .line
    display block

  .line:not(:first-child)
    position absolute

  .line:nth-child(1)
    animation clip 3000ms -300ms linear infinite, glitch1 1000ms -228ms linear infinite

  .line:nth-child(2)
    animation clip 3000ms -600ms linear infinite, glitch2 1000ms -927ms linear infinite

 @keyframes glitch1
  0%
	  transform translateX(0)
  80%
	  transform translateX(0)
	  color var(--outerspace)
  85%
