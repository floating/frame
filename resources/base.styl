.line:nth-child(8)
  animation clip 3000ms -2400ms linear infinite, glitch8 1000ms -380ms linear infinite

@keyframes glitch8
  0%
	  transform translateX(0)
  80%
	  transform translateX(0)
	  color var(--outerspace)
  85%
	  transform translateX(0px)
	  color var(--good)
  90%
	  transform translateX(-2px)
	  color var(--bad)
  95%
	  transform translateX(2px)
	  color var(--outerspace)
  100%
	  transform translateX(0)

.line:nth-child(9)
  animation clip 3000ms -2700ms linear infinite, glitch9 1000ms -863ms linear infinite

@keyframes glitch9
  0%
	  transform translateX(0)
  80%
	  transform translateX(0)
	  color var(--outerspace)
	85%
	  transform translateX(-3px)
	  color var(--good)
	90%
	  transform translateX(-1px)
	  color var(--bad)
	95%
	  transform translateX(-4px)
	  color var(--outerspace)
	100%
	  transform translateX(0)

.line:nth-child(10)
	animation clip 3000ms -3000ms linear infinite, glitch10 1000ms -745ms linear infinite

@keyframes glitch10
	0%
		transform translateX(0)
	80%
    transform translateX(0)
    color var(--outerspace)
	85%
    transform translateX(-1px)
    color var(--good)
	90%
    transform translateX(-3px)
    color var(--bad)
	95%
    transform translateX(-1px)
    color var(--outerspace)
	100%
		transform translateX(0)

@keyframes clip
  0%
    clip-path polygon(0 100%, 100% 100%, 100% 120%, 0 120%)
  100%
    clip-path polygon(0 -20%, 100% -20%, 100% 0%, 0 0)

@import './Native/style'
@import './Components'
@import './magic.styl'
